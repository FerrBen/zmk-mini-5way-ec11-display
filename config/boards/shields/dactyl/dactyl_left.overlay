#include "dactyl.dtsi"

&kscan0 {
    col-gpios
        = <&pro_micro 6 GPIO_ACTIVE_HIGH>  // col0
        , <&pro_micro 5 GPIO_ACTIVE_HIGH>  // col1
        , <&pro_micro 4 GPIO_ACTIVE_HIGH>  // col2
        , <&pro_micro 1 GPIO_ACTIVE_HIGH>  // col3 (moved from D3, D3 now used for I2C)
        , <&pro_micro 0 GPIO_ACTIVE_HIGH>  // col4 (moved from D2, D2 now used for I2C)
        ;
};

&split {
    status = "okay";
};
